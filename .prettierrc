{
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 120,
  "bracketSpacing": false,
  "arrowParens": "always",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^components/(.*)$",
    "^state/(.*)$",
    "^data/(.*)$",
    "^domain/(.*)$",
    "^[./]",
    "^/\\.css"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
